<table class="table table-bordered">
  <thead>
    <tr>
      <th width="5%">STT</th>
      <th>Tên</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Address</th>
      <th>Thêm học viên</th>
    </tr>
  </thead>
  <tbody>
    <% studentList.forEach((user, index) => { %>
    <tr>
      <td><%= index + 1 %></td>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.phone %></td>
      <td><%= user.address %></td>
      <td>
        <input class="checkboxItem" type="checkbox" value="<%= user.id %>" />
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
