<h3>Edit Document</h3>
<form action="" method="post">
  <div class="mb-3">
    <label for="">Tên Module</label>
    <input
      type="text"
      name="name"
      class="form-control"
      placeholder="Tên module..."
      value="<%= Modules.name %>"
    />
  </div>
  <% Modules.module_documents.forEach((item) => { %>
  <div class="mb-3">
    <label for="">Link tài liệu</label>
    <input
      type="text"
      name="pathName"
      class="form-control"
      placeholder="Link tài liệu..."
      value="<%= item.pathName %>"
    />
  </div>
  <% }) %>
  <div class="d-grid">
    <button class="btn btn-success">Lưu</button>
  </div>
</form>
